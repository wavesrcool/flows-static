{
  "name": "flows-static",
  "version": "1.0.0",
  "description": "Express.js file server",
  "main": "bin/index.js",
  "author": "Tyson Lupul <wavesrcool@icloud.com>",
  "license": "MIT",
  "scripts": {
    "_0lib": "rm -rf lib",
    "_0tscf": "rm -f tsconfig.tsbuildinfo",
    "w": "yarn _0lib && yarn _0tscf && tsc -w",
    "c": "yarn _0lib && yarn _0tscf && tsc",
    "build": "yarn && yarn lint && yarn c",
    "envs": "rm -f src/env.d.ts && gen-env-types .env -o src/env.d.ts -e .",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --cache --fix \"src/**/*.ts\"",
    "dev": "nodemon ./bin/index.js",
    "dev-eu": "nodemon ./bin/index.js eu",
    "start": "NODE_ENV=production node ./bin/index.js",
    "start-eu": "NODE_ENV=production node ./bin/index.js eu"

  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gen-env-types": "^1.3.4",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.6.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "helmet": "^5.1.1"
  }
}
